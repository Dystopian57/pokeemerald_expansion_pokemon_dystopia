script RivalWatchout {
	
	lockall
	setflag (FLAG_SAFE_FOLLOWER_MOVEMENT)
	checkplayergender
	if(var(VAR_RESULT) == MALE){createfieldmugshot (MUGSHOT_FPLAYER , EMOTE_ANGRY)}
	if(var(VAR_RESULT) == FEMALE){createfieldmugshot (MUGSHOT_MPLAYER , EMOTE_ANGRY)}
	setspeaker ([SP_NAME_RIVAL])
	playbgm (MUS_HELP, TRUE)
	applymovement (1, moves(face_down, emote_exclamation_mark))
	playse (SE_PIN)
	msgbox (format("{PLAYER}, don't go beyond the barricade!!"))
	removefieldmugshot
	checkplayergender
	if(var(VAR_RESULT) == MALE){createfieldmugshot (MUGSHOT_FPLAYER , EMOTE_NORMAL)}
	if(var(VAR_RESULT) == FEMALE){createfieldmugshot (MUGSHOT_MPLAYER , EMOTE_NORMAL)}
	msgbox (format("That Pokémon the Kantonians threw at us... it's too strong...!"))
	removefieldmugshot
	checkplayergender
	if(var(VAR_RESULT) == MALE){createfieldmugshot (MUGSHOT_FPLAYER , EMOTE_SAD)}
	if(var(VAR_RESULT) == FEMALE){createfieldmugshot (MUGSHOT_MPLAYER , EMOTE_SAD)}
	setspeaker ([SP_NAME_RIVAL])
	msgbox (format("Uggh... it got me good. Leave me... I'm done for!"))
	waitmessage
	applymovement (OBJ_EVENT_ID_PLAYER, moves(face_right))
	applymovement (1, moves(face_right))
	setvar(VAR_RIVALMIMIWARNINGDONE, 1)
	removefieldmugshot
	releaseall
	}

script Littleroot_Idletalk_Rival{
	lockall
	setflag (FLAG_SAFE_FOLLOWER_MOVEMENT)
	checkplayergender
	if(var(VAR_RESULT) == MALE){createfieldmugshot (MUGSHOT_FPLAYER , EMOTE_SAD)}
	if(var(VAR_RESULT) == FEMALE){createfieldmugshot (MUGSHOT_MPLAYER , EMOTE_SAD)}
	setspeaker ([SP_NAME_RIVAL])
	applymovement (1, moves(face_player))
	if (!flag (rivalhouse1stdialogue))
	{
	msgbox (format("Ugh... I don't have much longer... run away, while you can..."))
	removefieldmugshot
	applymovement (1, moves(face_player))
	setspeaker ([NULL])
	msgbox (format("{RIVAL} does not seem injured... and the 'enemy Pokémon' in question seems to be
	just a doll."))
	applymovement (OBJ_EVENT_ID_PLAYER, moves(face_right))
	msgbox (format("{RIVAL} seems to be having another episode... perhaps they'll calm down if the 'enemy'
	is dealt with."))
	setflag (rivalhouse1stdialogue)}
	else {msgbox(format("{PLAYER}... it's been an honour... to share battlefield... with you."))}
	applymovement (1, moves(face_right))
	removefieldmugshot
	releaseall
}

script LittlerootTown_MaysHouse_1F_EventScript_DOLL_BATTLE {
	lockall
    msgbox (format("As I thought, it's just a doll... one of Super Substitute, Devon's mascot. \p Wait... i-it's
	moving!?"))
    waitmessage
	setwildbattle(SPECIES_MIMIYURUI, 3)
	dowildbattle
	specialvar(VAR_RESULT, GetBattleOutcome)
	if(var(VAR_RESULT) == B_OUTCOME_CAUGHT){
		call(Won_or_Caught)
	}
	elif(var(VAR_RESULT) == B_OUTCOME_WON){
		call(Won_or_Caught)
	}
	elif(var(VAR_RESULT) == B_OUTCOME_RAN){
		call(RanAway)
	}
	elif(var(VAR_RESULT) == B_OUTCOME_PLAYER_TELEPORTED){
		call(RanAway)
	}
	waitmessage
	releaseall
}
 
script Won_or_Caught {
	setflag (FLAG_SAFE_FOLLOWER_MOVEMENT)
	applymovement (1, moves(walk_fast_down * 2, walk_fast_right * 3, walk_fast_up * 1, face_right))
	applymovement (OBJ_EVENT_ID_PLAYER, moves(face_left))
	hidefollower
	setspeaker ([SP_NAME_RIVAL])
	checkplayergender
	if(var(VAR_RESULT) == MALE){createfieldmugshot (MUGSHOT_FPLAYER , EMOTE_NORMAL)}
	if(var(VAR_RESULT) == FEMALE){createfieldmugshot (MUGSHOT_MPLAYER , EMOTE_NORMAL)}
	msgbox (format("{PLAYER}... you did it! You beat the fiend! \p This will go down in the history books.
	Let me restrain it - perhaps we can put it to some use."))
	waitmessage
	applymovement (OBJ_EVENT_ID_PLAYER, moves(walk_down * 1, face_up))
	applymovement (1, moves(walk_right * 1, face_up))
	waitmovement
	msgbox (format("Bang!"))
	waitmessage
	setobjectxy (3, 10, 6)
	playse (SE_BALL_OPEN)
	waitse
	removeobject(2)
	removeobject(3)
	applymovement (1, moves(face_down))
	setspeaker ([SP_NAME_RIVAL])
	msgbox (format("Piece of cake - now that this base is secure, let's push onto the north front!
	\pCover my rear, {PLAYER}. At the count of three, two, one..."))
	removefieldmugshot
	checkplayergender
	if(var(VAR_RESULT) == MALE){createfieldmugshot (MUGSHOT_FPLAYER , EMOTE_ANGRY)}
	if(var(VAR_RESULT) == FEMALE){createfieldmugshot (MUGSHOT_MPLAYER , EMOTE_ANGRY)}
	setspeaker ([SP_NAME_RIVAL])
	msgbox (format("CHAAAAAARGE!!"))
	waitmessage
	playse (SE_FLEE)
    applymovement (1, moves(walk_fast_left * 1, walk_fast_down * 1, walk_fast_left * 7, face_down))
    waitmovement (1)
    removeobject (1)
	playse (SE_DOOR)
	removefieldmugshot
    setflag(RIVAL_LEAVES_HOME)
    setflag(DOLL_BATTLE)
    setvar(VAR_LITTLEROOT_PHASE, 4)
	clearflag (Oldmantyhide)
	playbgm (MUS_LITTLEROOT_TEST, TRUE)
	return
}
 
script RanAway {
	lockall
	setspeaker ([SP_NAME_RIVAL])
	checkplayergender
	if(var(VAR_RESULT) == MALE){createfieldmugshot (MUGSHOT_FPLAYER , EMOTE_NORMAL)}
	if(var(VAR_RESULT) == FEMALE){createfieldmugshot (MUGSHOT_MPLAYER , EMOTE_NORMAL)}
	msgbox (format("{PLAYER}, don't falter now!! This...! \p This is the battle that will decide the fate
	of HOENN!"))
	removefieldmugshot
	checkplayergender
	if(var(VAR_RESULT) == MALE){createfieldmugshot (MUGSHOT_FPLAYER , EMOTE_ANGRY)}
	if(var(VAR_RESULT) == FEMALE){createfieldmugshot (MUGSHOT_MPLAYER , EMOTE_ANGRY)}
	msgbox (format("EVERYONE IS COUNTING ON YOU! YOU CAN DO IT"))
	removefieldmugshot
	releaseall
	return
}

