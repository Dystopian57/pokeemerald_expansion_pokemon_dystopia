mapscripts LittlerootTown_MapScripts {
    MAP_SCRIPT_ON_TRANSITION: NoRain}
	

script NoRain {

	if (var(VAR_LITTLEROOT_PHASE) == 5)
	{setweather (WEATHER_NONE)
}
	if (var(VAR_LITTLEROOT_PHASE) == 6)
	{setweather (WEATHER_NONE)
}

	if (var(VAR_LITTLEROOT_PHASE) == 7)
	{setweather (WEATHER_NONE)
}
}


script Littleroot_Techguy_Talk {
lockall
applymovement (3, moves(face_player))
if (flag(RIVAL_LEAVES_HOME) && !flag (Donutquestaccepted))
{msgbox (format("Oh, {PLAYER}! I've heard you're going to Oldale Town.\pIf you have time, would
you mind getting me a {COLOR RED}Crater Donut{COLOR DARK_GRAY}? They're to die for... \pIf you take a left on Route 101, you'll
arrive to the Great Crater. There's a guy that sells them there. \pDo me the favour, and I'll give
you some {COLOR RED}Pokballs{COLOR DARK_GRAY}. Fair deal, right?"))
setflag(Donutquestaccepted)
goto (accepteddonutquest)}
if (flag(RIVAL_LEAVES_HOME) && flag (Donutquestaccepted))
{msgbox (format("It's okay if you don't have time, but... I'd really like a taste of those donuts.
\p I'd go myself, but I have no Pokmon to protect me, and I got robbed by a {COLOR RED}Rebel{COLOR DARK_GRAY} last time I left
 town.\pBut you're a former soldier, right? They won't mess with you!"))}
else {msgbox (format("To think that Pokmon can be altered and made safer...\p
Technology is amazing, but it can be scary too."))}
accepteddonutquest:
releaseall
}

script Littleroot_Player_Mailbox {
lockall
	if (!flag (FLAG_ITEM_LITTLEROOT_MAILBOX))
	{msgbox (format("There's a package inside the mailbox. A letter is attached to it.
	\p For your invaluable service to the region, and the losses you suffered while performing it,
	please accept these tokens of gratitude, courtesy of Devon Corp."))
	playse (MUS_OBTAIN_ITEM)
	msgbox (format("Obtained the DEVONDEX!"))
	setflag(FLAG_SYS_POKEDEX_GET)
	waitbuttonpress
	msgbox (format("There's something else in the package..."))
	playse (MUS_OBTAIN_ITEM)
	msgbox (format("Obtained the DEVONAV!"))
	setflag(FLAG_SYS_POKENAV_GET)
	setflag(FLAG_ITEM_LITTLEROOT_MAILBOX)}
	else{msgbox (format("The mailbox is empty."))}
	releaseall
}

