script LittlerootTown_Trigger_NoEnterRival1{
	lockall
	msgbox (format("Someone is yelling inside..."))
	//checkplayergender
	//if(var(VAR_RESULT) == MALE){createfieldmugshot (MUGSHOT_FPLAYER , EMOTE_NORMAL)}
	//else {createfieldmugshot (MUGSHOT_MPLAYER , EMOTE_NORMAL)}
	msgbox (format("DAMN YOU, KANTONIANS! You will NOT TAKE HOENN FROM US!
	\p Take this..."))
	waitbuttonpress
	call(RivalShake)
	playse (SE_BANG)
	//removefieldmugshot
	//if(var(VAR_RESULT) == MALE){createfieldmugshot (MUGSHOT_FPLAYER , EMOTE_ANGRY)}
	//else {createfieldmugshot (MUGSHOT_MPLAYER , EMOTE_ANGRY)}
	msgbox (format("ORYAAAAAAAAHH!"))
	//removefieldmugshot
	msgbox (format("Whatever is happening inside, it does not look like a good time to visit...")) 
	waitmessage
	applymovement (OBJ_EVENT_ID_PLAYER, moves(walk_down * 1))
	waitmovement
	releaseall	
}

script RivalShake {
	setvar(VAR_0x8004, 2)  //@ vertical pan
	setvar(VAR_0x8005, 6)  //@ horizontal pan
	setvar(VAR_0x8006, 4)  //@ num shakes
	setvar(VAR_0x8007, 4)  //@ shake delay
	special(ShakeCamera)
 
}

script Littleroot_Idletalk_Oldman{
	lockall
	applymovement (1, moves(face_player))
	createfieldmugshot (MUGSHOT_BIRCH , EMOTE_NORMAL)
	setspeaker ([SP_NAME_BIRCH])
	if (var(VAR_LITTLEROOT_PHASE) == 0 && !flag(Birchafter1stgreeting))
	{msgbox (format("Ah, {PLAYER}! You're finally outside! I'm so glad to see you.\pPlease, go inside - Professor Balsam is waiting
	for you.\pI've retired, you see, and left her in charge of things."))
	setflag (Birchafter1stgreeting)
	goto (endfirstgreeting)}
	if (var(VAR_LITTLEROOT_PHASE) == 0 && flag(Birchafter1stgreeting))
	{msgbox (format("Take some shelter, {PLAYER}. Balsam has many things she wants to tell you about."))}
	if (var(VAR_LITTLEROOT_PHASE) == 3 && flag (checkonrival))
	{msgbox (format("Have you paid {RIVAL} a visit yet? Please... it would lift a weight off my old
	shoulders. \p I tried checking on them myself, but they wouldn't open the door.\p I hope you have
	better luck."))}
	if (var(VAR_LITTLEROOT_PHASE) == 5 && !flag (FLAG_HIDDEN_ITEM_ROUTE_101_VOLTORB_SHARD))
	{
	checkplayergender
	if(var(VAR_RESULT) == MALE){
	msgbox (format("How are the new crutches treating you, {PLAYER}?\pThe war left the terrain in a 
	treacherous state, so please take care."))}
	else
	{msgbox (format("How is the new arm sling treating you, {PLAYER}?\pThe war left the terrain in a 
	treacherous state, so please take care."))}}
	endfirstgreeting:

	if (var(VAR_LITTLEROOT_PHASE) == 5 && flag (FLAG_HIDDEN_ITEM_ROUTE_101_VOLTORB_SHARD))
	{msgbox (format("Oh, is that a Voltorb Shard? You should take that to Balsam's Aide at
	once."))

	}
	if(var(VAR_LITTLEROOT_PHASE) == 7)
	{msgbox (format("I know Professor Balsam and her Aide may seem eccentric sometimes...
	\p But they only have the best mind for the future of Hoenn. \p I hope you can find it in you
	to support them."))}

	removefieldmugshot
	releaseall
}

script LittlerootTown_Eventscript_Oldmanasking {
	lockall
	setflag (FLAG_SAFE_FOLLOWER_MOVEMENT)
	if (!flag (checkonrival))
		{playse (SE_PIN)
		applymovement (1, moves(emote_exclamation_mark))
		createfieldmugshot (MUGSHOT_BIRCH , EMOTE_NORMAL)
		setspeaker ([SP_NAME_BIRCH])
		msgbox (format("Ah, {PLAYER}!"))
		removefieldmugshot
		applymovement (1, moves(face_down, walk_down * 1, face_left))
		applymovement (OBJ_EVENT_ID_PLAYER, moves(face_right))
		waitmovement
		createfieldmugshot (MUGSHOT_BIRCH , EMOTE_NORMAL)
		setspeaker ([SP_NAME_BIRCH])
			if (!flag (GotPika))

			{bufferleadmonspeciesname(STR_VAR_1)
			msgbox (format("Such a cute {STR_VAR_1}! you have there! \p You see, I was wondering..."))}

			else 
			
			{msgbox (format("Such a cute Pur-... \p Wait, isn't that Balsam's Purachu? \p 
			Well, nevermind. I was wondering..."))}

		setspeaker ([SP_NAME_BIRCH])
		checkplayergender
		if(var(VAR_RESULT) == MALE){
		msgbox (format("Would you mind paying {RIVAL} a visit? She has been making quite a ruckus lately...\p
		It's as if she is reliving things that happened during the war.\pYou went to the front together, 
		didn't you? Perhaps you can calm her down."))}
		else {
		msgbox (format("Would you mind paying {RIVAL} a visit? He has been making quite a ruckus lately...\p
		It's as if he is reliving things that happened during the war.\pYou went to the front together, 
		didn't you? Perhaps you can calm him down."))}
		removefieldmugshot
		applymovement (1, moves(walk_up * 1, face_down))
		waitmovement
		setflag (checkonrival)}
	else {setflag (checkonrival)}
	releaseall

}

script OldManThanks {
	lockall
	setflag (FLAG_SAFE_FOLLOWER_MOVEMENT)
	msgbox (format("Ah, {PLAYER}!"))
	removefieldmugshot
	setobjectxy (1, 14, 16)
	applymovement (1, moves(walk_up * 5))
	waitmovement
	createfieldmugshot (MUGSHOT_BIRCH , EMOTE_NORMAL)
	setspeaker ([SP_NAME_BIRCH])
	msgbox (format("I saw {RIVAL} storming out like a frenzied Linoone! What got into them?\p Well, at
	least you got them to leave the house. That may do them some good."))
	checkplayergender
	if(var(VAR_RESULT) == MALE){
		msgbox (format("Thank you for checking on her. Here,
		take this as a form of thanks."))}
		else {
		msgbox (format("Thank you for checking on him. Here,
		take this as a form of thanks."))}
	removefieldmugshot
	setspeaker ([NULL])
	playfanfare(MUS_OBTAIN_ITEM)
	setflag (FLAG_SYS_B_DASH)
	checkplayergender
		if(var(VAR_RESULT) == MALE)
		{msgbox (format("Obtained the Better Crutches!"))
		waitmessage
		createfieldmugshot (MUGSHOT_BIRCH , EMOTE_NORMAL)
		setspeaker ([SP_NAME_BIRCH])
		msgbox (format("These are some old crutches of mine.\pI noticed yours are not in the best shape - these
		should help you move around better."))}
		else
		{msgbox (format("Obtained the Better Arm Sling!"))
		waitmessage
		createfieldmugshot (MUGSHOT_BIRCH , EMOTE_NORMAL)
		setspeaker ([SP_NAME_BIRCH])
		msgbox (format("This is an old arm sling of mine.\pI noticed yours is not in the best shape - this one
		should secure your arm better, so you can move around more easily."))}
		setspeaker ([SP_NAME_BIRCH])
		msgbox (format("Well, take care on your trip to Oldale now!"))
		setspeaker ([SP_NAME_BIRCH])
		applymovement (1, moves(face_left))
		msgbox (format("There's much hardship in these parts lately, and people
		are not as friendly as they used to."))
		setspeaker ([SP_NAME_BIRCH])
		applymovement (1, moves(face_up))
		msgbox (format("But have faith in your Pokmon, and you should be fine. Good bye!"))
		closemessage
		applymovement (1, moves(walk_down * 7))
		waitmovement (1)
		removeobject (1)
		setobjectxy (1, 4, 17)
		setvar(VAR_LITTLEROOT_PHASE, 5)
		removefieldmugshot
	//Receive Running shoes
	releaseall}

